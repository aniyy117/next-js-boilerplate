{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off"
  },
  "ignorePatterns": ["**/node_modules/**", "**/.next/**"],
  "overrides": [
    {
      "files": ["**/*.jsx", "**/*.js"]
    }
  ]
}
